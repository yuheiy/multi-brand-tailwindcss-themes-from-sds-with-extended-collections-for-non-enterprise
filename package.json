{
  "name": "sds-with-extended-collections-to-tailwindcss-for-non-enterprise",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node --run build:sd && pnpm run -r build",
    "build:sd": "node scripts/build-tailwind-tokens.ts && style-dictionary build --config sd.config.ts",
    "dev": "pnpm run -r --parallel dev"
  },
  "devDependencies": {
    "@types/node": "^24.10.13",
    "es-toolkit": "^1.44.0",
    "map-obj": "^6.0.0",
    "oxfmt": "^0.34.0",
    "style-dictionary": "^5.3.2",
    "tailwind-merge": "^3.5.0",
    "typescript": "~5.9.3"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^24.13.1",
      "onFail": "download"
    }
  },
  "packageManager": "pnpm@10.30.1"
}
